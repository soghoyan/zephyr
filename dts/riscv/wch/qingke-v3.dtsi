#include <zephyr/dt-bindings/interrupt-controller/ch32v-qingke-v3-int.h>

/ {
	cpus {
		#address-cells = <1>;
		#size-cells = <0>;

		cpu0: cpu@0 {
			compatible = "wch,qingke-v3";
			reg = <0>;
		};
	};

	soc {
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <1>;
	};
};
